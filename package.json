		"autoprefixer": "^10.4.7",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.14",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"prismjs": "^1.28.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.6",
		"typescript": "^4.7.4",
